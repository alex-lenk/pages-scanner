{
  "name": "pages-scanner",
  "version": "2.3.1",
  "description": "Scans for page names and saves them.",
  "main": "./PagesScannerWebpackPlugin.cjs",
  "exports": {
    ".": [
      {
        "import": "./PagesScannerWebpackPlugin.mjs",
        "require": "./PagesScannerWebpackPlugin.cjs"
      },
      "./PagesScannerWebpackPlugin.cjs"
    ]
  },
  "module": "./PagesScannerWebpackPlugin.mjs",
  "type": "module",
  "devDependencies": {
    "@babel/cli": "^7.22.10",
    "@babel/preset-env": "^7.22.10"
  },
  "peerDependencies": {
    "webpack": "5.x"
  },
  "bin": {
    "scanPages": "./index.js"
  },
  "scripts": {
    "scanPages": "scanPages",
    "build": "npx babel PagesScannerWebpackPlugin.mjs -o PagesScannerWebpackPlugin.cjs"
  },
  "keywords": [
    "pages",
    "scanner",
    "pages-scanner"
  ],
  "standard": {
    "ignore": [
      "**/example/**"
    ],
    "globals": [
      "it"
    ]
  },
  "author": "AlexLenk.ru",
  "dependencies": {
    "glob": "^10.3.3"
  },
  "files": [
    "build",
    "index.mjs",
    "!*.d.ts"
  ],
  "engines": {
    "node": ">=12"
  },
  "license": "ISC"
}
